{
  "name": "@vkontakte/icons",
  "version": "1.159.0",
  "description": "SVG icons, provided as a React components. With love from VKontakte",
  "repository": "https://github.com/VKCOM/icons",
  "homepage": "https://vkcom.github.io/icons/",
  "author": "bumbledos@gmail.com",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "dist",
    "ts",
    "src/svg"
  ],
  "main": "dist/index.js",
  "module": "dist/es6/index.js",
  "typings": "dist/typings/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "engines": {
    "yarn": ">=1.21.1 <=1.22.18"
  },
  "scripts": {
    "docs": "node scripts/docs",
    "build": "yarn icons-build && yarn docs",
    "icons-tsc": "NODE_ENV=production tsc --emitDeclarationOnly --declaration --project tsconfig-icons.json --outDir dist/typings",
    "swc-build-cjs": "swc ts/ -d dist/ -C jsc.target=es5 -C module.type=commonjs",
    "swc-build-es6": "swc ts/ -d dist/es6/ -C jsc.target=es5",
    "swc-build-all": "yarn swc-build-es6 && yarn swc-build-cjs",
    "icons-build": "node scripts/icons.js && yarn swc-build-all && yarn icons-tsc"
  },
  "dependencies": {
    "svg-baker-runtime": "1.4.7"
  },
  "peerDependencies": {
    "@types/react": "^16.9.34 || ^17.0.0",
    "react": "^16.9.0 || ^17.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.177",
    "@types/react": "16.9.34",
    "css-loader": "5.2.4",
    "file-loader": "6.2.0",
    "glob": "7.1.6",
    "html-webpack-plugin": "5.3.1",
    "react": "^16.9.0",
    "react-color": "2.18.0",
    "react-dom": "^16.9.0",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "svg-baker": "1.7.0",
    "svgo": "2.8.0",
    "swc-loader": "^0.2.0",
    "typescript": "^4.0.2",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-merge": "4.2.2"
  },
  "browserslist": [
    "android >= 4.4",
    "ios >= 9"
  ]
}
